{
  "name": "welcomely",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "g:entity": "yarn typeorm entity:create",
    "g:migration": "yarn typeorm migration:generate -d db/data-source.ts",
    "db:migrate": "yarn typeorm migration:run -d db/data-source.ts",
    "db:show": "yarn typeorm migration:show -d db/data-source.ts",
    "typeorm": "yarn conf ./node_modules/typeorm/cli.js",
    "conf": "DOTENV_CONFIG_PATH=./.env ts-node -P ./tsconfig.yarn.json -r dotenv/config"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "mysql": "^2.18.1",
    "next": "13.5.4",
    "pg": "^8.13.0",
    "preline": "^2.4.1",
    "react": "^18",
    "react-dom": "^18",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/node": "^16.11.10",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10",
    "postcss": "^8",
    "tailwindcss": "^3",
    "ts-node": "^8.4.1",
    "typescript": "^5"
  }
}
